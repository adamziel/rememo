{
	"root": true,
	"extends": "plugin:@wordpress/eslint-plugin/es5",
	"parserOptions": {
		"ecmaVersion": 2015,
		"sourceType": "module"
	},
	"settings": {
		"jsdoc": {
			"preferredTypes": {
				"object": "object"
			},
			"mode": "typescript"
		}
	},
	"env": {
		"node": true
	},
	"globals": {
		"WeakMap": true
	},
	"rules": {
		"one-var": "off"
	}
}
